{
  "name": "unique-recruitment-candidate-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "extract": "NODE_ENV=development lingui extract --overwrite",
    "compile": "lingui compile",
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "prebuild": "rimraf .next",
    "prepare": "husky install"
  },
  "dependencies": {
    "@lingui/core": "^3.8.10",
    "@lingui/detect-locale": "^3.8.10",
    "@lingui/react": "^3.8.10",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@reduxjs/toolkit": "^1.5.1",
    "@uniqs/api": "^0.1.0",
    "@uniqs/config": "^0.0.3",
    "@uniqs/storage": "^0.1.1",
    "@uniqs/utils": "^0.1.0",
    "clsx": "^1.1.1",
    "next": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.0.4",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@lingui/cli": "^3.8.10",
    "@lingui/loader": "^3.8.10",
    "@lingui/macro": "^3.8.10",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "babel-plugin-macros": "^3.0.1",
    "rimraf": "^3.0.2"
  },
  "lingui": {
    "locales": [
      "zh",
      "en"
    ],
    "catalogs": [
      {
        "path": "src/locales/{locale}/messages",
        "include": [
          "src"
        ]
      }
    ],
    "format": "po"
  }
}
